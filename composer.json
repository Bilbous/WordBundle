{
    "name": "Bilbous/wordbundle",
    "description": "This is a Symfony2 Bundle helps you to read and write Word files (including pdf, docx, odt), thanks to the PHPWord library",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Yannick Cenatiempo",
            "email": "ycenatiempo@gmail.com"
        },
        {
            "name": "WordBundle Contributors",
            "homepage": "https://github.com/Bilbous/WordBundle#contributors"
        }
    ],

    "keywords": ["docx","Word", "symfony2", "bundle"],
    "homepage": "http://www.welcometothebundle.com",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.6|~3.0",
        "phpoffice/phpword": "~0.13.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.6",
        "symfony/finder": "~2.6|~3.0",
        "symfony/form": "~2.6|~3.0",
        "symfony/class-loader": "~2.6|~3.0",
        "symfony/validator": "~2.6|~3.0",
        "symfony/browser-kit": "~2.6|~3.0",
        "sensio/framework-extra-bundle": "~2.3|~3.0"
    },

    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "Bilbous\\WordBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bilbous\\WordBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
